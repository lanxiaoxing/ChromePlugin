{
	"manifest_version": 2,

	"name": "QRCode Plugin",
	"description": "This extension shows a QRCode image for the current page",
	"version": "1.0",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "click for generating QRCode!"
	},

	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}